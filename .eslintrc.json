{
  "env": {
    "browser": true,
    "es2022": true
  },
  "extends": ["airbnb-base", "plugin:json/recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "root": true,
  "rules": {
    "no-underscore-dangle": "off",
    "no-param-reassign": "off",
    "prefer-destructuring": "off"
  }
}
