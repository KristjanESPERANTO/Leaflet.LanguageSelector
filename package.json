{
  "name": "@kristjan.esperanto/leaflet-language-selector",
  "version": "3.0.0-alpha.1",
  "description": "A language selector for Leaflet based maps.",
  "main": "src/leaflet.languageselector.js",
  "exports": {
    ".": {
      "import": "./src/leaflet.languageselector.js",
      "default": "./src/leaflet.languageselector.js"
    },
    "./src/leaflet.languageselector.css": "./src/leaflet.languageselector.css"
  },
  "files": [
    "src/",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "images/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KristjanESPERANTO/Leaflet.LanguageSelector.git"
  },
  "author": "buche",
  "license": "CC0-1.0",
  "keywords": [
    "leaflet",
    "plugin",
    "language",
    "i18n",
    "map",
    "control"
  ],
  "peerDependencies": {
    "leaflet": ">=2.0.0-alpha.1"
  },
  "bugs": {
    "url": "https://github.com/KristjanESPERANTO/Leaflet.LanguageSelector/issues"
  },
  "homepage": "https://github.com/KristjanESPERANTO/Leaflet.LanguageSelector#readme",
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@stylistic/eslint-plugin": "^5.5.0",
    "eslint": "^9.38.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsonc": "^2.21.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "stylelint": "^16.25.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-prettier": "^5.0.3"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.css": "stylelint --fix"
  },
  "scripts": {
    "lint": "eslint && stylelint **/*.css && prettier --check .",
    "lint:fix": "eslint --fix && stylelint --fix **/*.css && prettier --write .",
    "test": "npm run lint",
    "prepare": "husky"
  },
  "sideEffects": false,
  "type": "module"
}
